         <Units>arcsec</Units>
         <UCD version="1+">phys.angSize.sminAxis</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.pa" indexed="false">
         <Name>pa</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[position angle (east of north) derived from the elliptical Gaussian model for the source]]></Description>
         <Units>degrees</Units>
         <UCD version="1+">pos.posAng</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.fMaj" indexed="false">
         <Name>fMaj</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[major axes derived from the elliptical Gaussian model for the source before deconvolution.]]></Description>
         <Units>arcsec</Units>
         <UCD version="1+">phys.angSize.smajAxis</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.fMin" indexed="false">
         <Name>fMin</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[minor axes derived from the elliptical Gaussian model for the source before deconvolution.]]></Description>
         <Units>arcsec</Units>
         <UCD version="1+">phys.angSize.sminAxis</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.fPA" indexed="false">
         <Name>fPA</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[position angle (east of north) derived from the elliptical Gaussian model for the source before deconvolution.]]></Description>
         <Units>degrees</Units>
         <UCD version="1+">pos.posAng</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
   </Table>
   </Catalog>
</DatasetDescription>

