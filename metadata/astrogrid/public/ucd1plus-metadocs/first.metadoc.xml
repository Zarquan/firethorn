<?xml version="1.0" encoding="UTF-8"?>
<DatasetDescription xmlns='urn:astrogrid:schema:dsa:TableMetaDoc:v1.1'
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <Catalog ID="first">
   <Name>FIRST</Name>
   <Description></Description>
   <Table ID="firstSource">
      <Name>firstSource</Name>
      <Description><![CDATA[ Contains the parameters provided for each source in the FIRST Survey Catalog: 03Apr11 Version]]></Description>
      <ConeSettings>
          <RAColName>RA</RAColName>
	  <DecColName>DEC</DecColName>
      </ConeSettings>      
      <Column ID="firstSource.fieldName" indexed="false">
         <Name>fieldName</Name>
         <Datatype>string</Datatype>
         <Description><![CDATA[Name of the coadded image containing the source (encodes the position of the field center).]]></Description>
         <Units> </Units>
         <UCD version="1+">obs.image</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.seqNo" indexed="true">
         <Name>seqNo</Name>
         <Datatype>string</Datatype>
         <Description><![CDATA[sequential object number]]></Description>
         <Units> </Units>
         <UCD version="1+">meta.id;meta.main</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.cx" indexed="false">
         <Name>cx</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[unit vector of spherical co-ordinates]]></Description>
         <Units> </Units>
         <UCD version="1+">pos.distance;pos.cartesian.x</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.cy" indexed="false">
         <Name>cy</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[unit vector of spherical co-ordinates]]></Description>
         <Units> </Units>
         <UCD version="1+">pos.distance;pos.cartesian.y</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.cz" indexed="false">
         <Name>cz</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[unit vector of spherical co-ordinates]]></Description>
         <Units> </Units>
         <UCD version="1+">pos.distance;pos.cartesian.z</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.htmID" indexed="false">
         <Name>htmID</Name>
         <Datatype>long</Datatype>
         <Description><![CDATA[Hierarchical Triangular Mesh (HTM) index, 20 deep, for equatorial co-ordinates]]></Description>
         <Units> </Units>
         <UCD version="1+">pos.eq</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.ra" indexed="false">
         <Name>ra</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[J2000 Celestial Right Ascension]]></Description>
         <Units>deg</Units>
         <UCD version="1+">pos.eq.ra;meta.main</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.dec" indexed="false">
         <Name>dec</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[J2000 Celestial Declination]]></Description>
         <Units>deg</Units>
         <UCD version="1+">pos.eq.dec;meta.main</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.warnFlag" indexed="false">
         <Name>warnFlag</Name>
         <Datatype>string</Datatype>
         <Description><![CDATA[Warning flag indicating that the source may be a sidelobe of a nearby bright source.]]></Description>
         <Units> </Units>
         <UCD version="1+">meta.code</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.fPeak" indexed="false">
         <Name>fPeak</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[peak flux densities]]></Description>
         <Units>mJy</Units>
         <UCD version="1+">phot.flux.density;em.radio.750-1500MHz</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.fInt" indexed="false">
         <Name>fInt</Name>
         <Datatype>double</Datatype>
         <Description><![CDATA[integrated flux densities]]></Description>
         <Units>mJy</Units>
         <UCD version="1+">phot.flux.density;em.radio.750-1500MHz</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.rms" indexed="false">
         <Name>rms</Name>
