         <Units>arcsec</Units>
         <UCD version="1+">phys.angSize.smajAxis</UCD>
         <ErrorColumn></ErrorColumn>
      </Column>
      <Column ID="firstSource.min" indexed="false">
         <Name>min</Name>
         <Datatype>double</Datatype>
